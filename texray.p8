--update

function update_game()
 
 p.sx=0
 p.sy=0
 p.i=2
 p.v=max(p.v-1,0)
 
 updateprt()
 
 if #mobs==0 then
 for i=1,8 do
  mob_spawner(mp,rnd(120),-8)
 end
 end
 --animate flame
 animate(f)

 for m in all(mobs) do
  if rnd()<.01 then
   m.sx*=-1
  end

  local dx=p.x-m.x
  local dy=p.y-m.y
  local tar=atan2(dy,dx)
  m.sy+=cos(tar)*0.05
  m.sx+=sin(tar)*0.05
 end
 --animate muzzle flash
 muzzle-=1
 
 shoot_timer=max(shoot_timer-1,0)
 
 --controls
 if (btn(0)) then
  p.sx=-2
  p.i=1
 end
 
 if (btn(1)) then
  p.sx=2
  p.i=3
 end
 
 if (btn(2)) then
  p.sy=-2
 end
 
 if (btn(3)) then
  p.sy=2
 end
 
 if btnp(4) then
  bp=1+bp%#bt
 end 
 
 if (btn(5)and shoot_timer<=0)shoot(bp)
  
 
 --collisions
 if (p.x < 0) then
 	p.x=0
 end
 
 if (p.x > 120) then
 	p.x=120
 end

 if (p.y < 0) then
 	p.y=0
 end
 
 if (p.y > 120) then
 	p.y=120
 end 
 
 for m in all (mobs) do
	 --collisions
		if p.v==0 and cln(p,m) then
			if p.v==0 then
				lives-=1
				p.v=90
			end
			if lives<=0 then
				mode="over"
   	return
   end
  end
  for b in all(bulets) do
	  if cln(b,m) then
			 score+=1000
	  end
		end
 end 
 
 
 --steps
 p.x+=p.sx
 p.y+=p.sy

 animatestars()
 
 update_objects(mobs)
 update_objects(bulets)
end

 function update_start()
 animatestars(10)
  if btnp(4) or btnp(5) then
  start_game()
  end
end

function update_over()
 if btnp(4) or btnp(5) then
  mode="start"
 end
end


function update_menu()
 aintro=10-m_timer*0.06
 animatestars(max(1,aintro));
 
 for s in all(squad) do
  s.y+=s.sy
  s.x+=s.sx
  -- halt squad
  if (p.y==60) p.sy=0 
 end
 
 if (m_timer>160) animate(f)
 if m_timer>200 then
  for i=2,#squad do
   local s=squad[i]
	  s.sx=i%2==0and-1or 1
	  s.i=i%2==0and 33 or 35
  end
 end 
 
 m_timer+=1
 if m_timer>250 then
  mode="game"
  m_timer=0
 end
end

function animate(o)
 if (o.ais==nil) o.ais=1 
 o.ai=o.icn+time()*o.ais%o.frm
end

function update_objects(objects)
	for b in all(objects) do
	  --animate objects
	  animate(b,4)
	  --move objects
	  b.x+=b.sx
	  b.y+=b.sy
	  if b.typ==3 then
	   prtgen(1,b.x+4,b.y+4)
	  end
	  
	  --clean objects
	  if b.x<-8or b.x>129
	     or b.y<-8or b.y>129then
	   del(objects,b)
	  end
	 end
end
