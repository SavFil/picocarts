--tools

function draw_starfield()
 for s in all(stars) do
  pset(s.x,s.y,s.cl)
 end
end

function animatestars(f)
 if(f==nil)f=1
 for s in all(stars) do
  s.y+=s.spd*f
  if(s.y>128)s.y-=128
 end
end

function shoot(_b)
 local b=bt[_b]
 sfx(b.typ)  
 muzzle=b.m
 shoot_timer=b.d
 local r=b.mlt%2==0 and 100 or 90
 local s=-1
 if b.typ!=3 then
 for i=0,b.mlt-1 do
  local t={}
  s*=-1
  r+=s*i*20
  
  objcopy(b,t)
	 t.x=p.x
	 t.y=p.y-8
	 t.ai=b.icn
	 if b.mlt>1 then
	  t.sx=cos(r/360)*2
	  t.sy=sin(r/360)*2
	 end
	 
	 add(bulets,t)
	end
	else
	 for i=b.mlt,b.mlt+b.mlt do
		 local z,r,s={},(i*i)%360/360,
		 i/b.mlt*1.5
	  objcopy(b,z)
   
		 z.x=p.x
		 z.y=p.y-8
		 z.ai=b.icn
	  z.sx=cos(r)*s
   z.sy=sin(r)*s
	  add(bulets,z)
	 end
	end
end

function blink()
 return flr(-cos(t()*1.5)+0.2)+6
end

function draw_logo(i,x,y)
 sx,sy=(i%16)*8,flr(i\16)*8
 sspr(sx,sy,88,32,x,y)
end

function printc(_str,_y,_c)
 len=#_str
 where=63-(len*2)
 if (where<0) where=0
 print(_str,where,_y,_c)
end

function mob_spawner(_mp,_x,_y)
  local m={}
  objcopy(mobt[_mp],m)
  m.x=_x
  m.y=_y
  m.sx=0
  m.sy=0.5+rnd()
  m.ai=m.icn
  
  if rnd()<.3 then
   m.sx=-.3
  end
  add(mobs,m)
end

function objcopy(_a,_b)
 for k,v in pairs(_a) do
	 _b[k]=v
	end
end


function addprt(_tex,_x,_y,_xv,_yv,_ang,_angv,_clr,_size,_ttl)
 local p={
  tex=_tex,
  x=_x,
  y=_y,
  xv=_xv,
  yv=_yv,
  vel=_vel,
  --ang=_ang,
  --angv=_angv,
  clr=_clr,
  size=_size,
  ttl=_ttl
 }
 
 add(particle,p)
end

function updateprt()
 for p in all(particle) do
  if p.ttl<=0 then
   del(particle,p)
  else 
	  p.ttl-=1
	  --p.x+=p.xv;
	  --p.y+=p.yv;
	  p.x+=p.xv--+sin((time())*0.25)+sin((time())*0.5)
	  p.y+=p.yv--4
	 end
  --p.ang+=p.angv;
 end
end

function drawprt()
 for p in all(particle)do
  --pal(6,p.clr)
  --spr(p.tex,p.x,p.y)
  --pset(p.x,p.y,p.clr)
  --rectfill(p.x,p.y,p.x+p.size,p.y+p.size,p.clr)
  circfill(p.x,p.y,p.size+p.ttl,p.clr)
  
  --pal()
 end
  --local p=particle[#particle]
 -- circfill(p.x,p.y,5-sin(time()/1.2),6+sin(time()*2))
end

function prtgen(_prts,_x,_y)
  local prts=mid(1,_prts,50) 
  
--  debug[1]=prts
 
  
  for i=1,prts do
	  local tex=1//+flr(rnd(#texture))
		 local x=_x
		 local y=_y
		 local xv=rnd(2)-1
		 local yv=rnd(2)-1
		 local c=8+flr(rnd(2))
		 local size=0
		 local ttl=2+rnd(4)
		 addprt(tex,x,y,xv,yv,0,0,c,size,ttl)
  end
end

function cln(_a,_b)

 if(_a.x+7<_b.x)return false
 if(_b.x+7<_a.x)return false
 if(_a.y+7<_b.y)return false
 if(_b.y+7<_a.y)return false

 sfx(10)
 del(mobs,_b)
 return true
end


