version 36
__lua__
function _init()
	cls(0)
 m_timer=0
 mode="start"
 
 stars={}
 for i=1,30 do
  local s={}
  s.x=rnd(128)
  s.y=rnd(128)
  s.spd=rnd(1.5)+.5
  s.ant=rnd(200)+15
  s.cl=s.spd>1.25and 14
  						or s.spd>1and 2
  						or 1
  add(stars,s)
  										
 end
end

function _update()
 if mode=="game" then
 	update_game() 
 elseif mode=="start" then
  update_start()
 elseif mode=="over" then
  update_over()
 elseif mode=="menu" then
  update_menu()
 end
end

function _draw()
 
 if mode=="game" then
 	draw_game() 
 elseif mode=="start" then
  draw_start()
 elseif mode=="over" then
  draw_over()
 elseif mode=="menu" then
  draw_menu()
 end
end

function start_game()
 p={x=60,y=130,sx=0,sy=-0.5,i=2,v=0}
 bt={{typ=0,sx=0,sy=-2,icn=9,d=10,m=6,frm=5,mlt=1,ais=7},
    {typ=1,sx=0,sy=-5,icn=14,d=4,m=3,frm=1,mlt=1,ais=5},
    {typ=2,sx=0,sy=0,icn=27,d=30,m=10,frm=5,mlt=20,ais=7},
    {typ=3,sx=0,sy=0,icn=45,d=100,m=10,frm=1,mlt=60}
    }
 bp=1
 bulets={}
 
 squad={p,
 						{x=40,y=140,sx=0,sy=-0.5,i=34},
 						{x=80,y=140,sx=0,sy=-0.5,i=34},
 						{x=20,y=150,sx=0,sy=-0.5,i=34},
 						{x=100,y=150,sx=0,sy=-0.5,i=34}
 					 	}
 					 	
 mobt={{icn=64,frm=5,ais=6}}
 mp=1
 mobs={}
 aintro=10
 shoot_timer=0
 
 
 f={icn=4,ai=4,frm=5,ais=12}
	
	particle={}
	muzzle=0 

 score=10000
 lives=3
 bombs=2
 
 
 mode="game"
end
